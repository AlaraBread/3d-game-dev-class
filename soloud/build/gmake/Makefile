all: $(PROJECTS)

SoloudStatic: 
	@echo "==== Building SoloudStatic ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f SoloudStatic.make

SoloudDynamic: SoloudStatic
	@echo "==== Building SoloudDynamic ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f SoloudDynamic.make

clean:
	@${MAKE} --no-print-directory -C . -f SoloudStatic.make clean
	@${MAKE} --no-print-directory -C . -f SoloudDynamic.make clean

